                            color: 'blue', 
                            fillColor: 'blue', 
                            fillOpacity: 0.4 ,     
                            info: infos })


                        var fleachenErstellt = { flaechenID: flaechenID, nmin: nmin, smin: smin, humus: humus, cn: cn};



